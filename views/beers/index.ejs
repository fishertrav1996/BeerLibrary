<h2>Search Beers</h2>
<form action="/beers" method="get">
    <label>Name</label>
    <input type="text" name="name" value="<%= filters.name %> ">
    <button type="submit">Search</button>
</form>
<br>
<div>
    <% beers.forEach(beer => {  %> 
            <img height="150" width="100" src="<%= beer.imagePath %> ">
            <%= beer.name %>
    <% }) %>
</div>